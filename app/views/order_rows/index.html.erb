<p id="notice"><%= notice %></p>

<h1>Listing Order Rows</h1>

<table>
  <thead>
  <tr>
    <th>Paid</th>
    <th>Net</th>
    <th>Order code</th>
    <th>Transaction</th>
    <th>Payment</th>
    <th>Client ip</th>
    <th>Verified</th>
    <th>Created by admin</th>
    <th>Client</th>
    <th>Service</th>
    <th colspan="3"></th>
  </tr>
  </thead>

  <tbody>
  <% @order_rows.each do |order_row| %>
    <tr>
      <td><%= order_row.paid %></td>
      <td><%= order_row.net %></td>
      <td><%= order_row.order_code %></td>
      <td><%= order_row.transaction_id %></td>
      <td><%= order_row.payment_id %></td>
      <td><%= order_row.client_ip %></td>
      <td><%= order_row.verified %></td>
      <td><%= order_row.created_by_admin %></td>
      <td><%= order_row.client %></td>
      <td><%= order_row.service %></td>
      <td><%= link_to 'Show', order_row %></td>
      <td><%= link_to 'Edit', edit_order_row_path(order_row) %></td>
      <td><%= link_to 'Destroy', order_row, method: :delete, data: {confirm: 'Are you sure?'} %></td>
    </tr>
  <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Order row', new_order_row_path %>
